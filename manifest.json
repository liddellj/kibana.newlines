{
  "manifest_version": 2,

  "name": "Kibana Newlines",
  "description": "Converts instances of '/r/n' into newlines within log messages in the Kibana interface.",
  "version": "1.0",
  "permissions": [
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "content/jquery-1.8.0.min.js",
        "content/kibana.newlines.js"
      ],
      "run_at": "document_end"
    }
  ]
}